    "def find_max_gift(budget,prices):\n",
    "    prices.sort()\n",
    "    gift = 0\n",
    "    for price in prices:\n",
    "        if budget >= price:\n",
    "            budget -= price \n",
    "            gift += 1\n",
    "        else:\n",
    "            break\n",
    "    return gift\n",
    "\n",
    "gift = find_max_gift(1000,[300, 200, 400, 500])\n",
    "print(gift)"
   ]
  }
